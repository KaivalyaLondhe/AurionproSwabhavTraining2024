package com.aurionpro.creational.factory.model;

public class SavingsAccount extends IAccount{
	private double minimumBalance;
	public SavingsAccount(long accountNumber, String name, double balance, double minimumBalance) {
		super(accountNumber, name, balance);
		this.minimumBalance = minimumBalance;
		
		// TODO Auto-generated constructor stub
	}
	public double getMinimumBalance() {
		return minimumBalance;
	}
	public void setMinimumBalance(double minimumBalance) {
		this.minimumBalance = minimumBalance;
	}
	@Override
	public String toString() {
		return "SavingsAccount [minimumBalance=" + minimumBalance + ", getAccountNumber()=" + getAccountNumber()
				+ ", getName()=" + getName() + ", getBalance()=" + getBalance() + "]";
	}
	@Override
	public double credit(double amount) {
		// TODO Auto-generated method stub
		
	}
	@Override
	public double debit(double amount) {
		// TODO Auto-generated method stub
		return 0;
	}
	

}
