package com.aurionpro.ocp.violation.model;

public class InterestRateCalculator {
	FixedDeposit fixedDeposit = new FixedDeposit(int accountNumber, String name, double principle, int duration, FestivalType festival);
	
	public double getInterestRates(int choice) {
		switch(choice) {
		case 1: 
			return 8;
		case 2: 
			return 8.5;
		case 3: 
			return 7.5;
		case 4:
			return 6.5;
		}
		
	}
}
